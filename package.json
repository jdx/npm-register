  ],
  "license": "ISC",
  "main": "lib/start.js",
  "repository": "jdxcode/npm-register",
  "scripts": {
    "build": "webpack --config ./config/webpack.config",
    "server": "node bin/npm-register start",
    "server:dev": "nodemon --watch lib bin/npm-register start",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others -p \"[{name}]\" -n \"API,UI\" -c \"bgMagenta.bold,bgGreen.bold\" \"npm run server:dev\" \"webpack-dev-server --watch-poll --colors --config ./config/webpack.config\"",
    "start": "cross-env NODE_ENV=production concurrently --kill-others-on-fail -p \"[{name}]\" -n \"UI,API\" -c \"bgMagenta.bold,bgGreen.bold\" \"npm run build\" \"npm run server\"",
    "test": "nyc mocha && standard"
  },
  "standard": {
    "env": "mocha"
  }
}
